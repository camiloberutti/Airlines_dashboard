"mask = (df['WEATHER_DELAY'] >= 4.03) & (df['WEATHER_DELAY'] <= 4.04)\n","\n","# Get the count of such values\n","num_to_replace = mask.sum()\n","\n","if num_to_replace > 0:\n","    # Generate random values from a normal distribution\n","    # Choose a mean and standard deviation that represent a \"normal variation\" for delays.\n","    # For example, mean=15 minutes, std=5 minutes.\n","    # Ensure delays are not negative by clamping at 0.\n","    random_values = np.random.normal(loc=15.0, scale=5.0, size=num_to_replace)\n","    random_values[random_values < 0] = 0\n","\n","    # Assign these random values to the 'WEATHER_DELAY' column for the identified rows\n","    df.loc[mask, 'WEATHER_DELAY'] = random_values\n","\n","print(f\"Replaced {num_to_replace} weather delays between 4.03 and 4.04 with random values (mean=15, std=5).\")"],"metadata":{"id":"FldHzZ0FhGbw","colab":{"base_uri":"https://localhost:8080/"},"executionInfo":{"status":"ok","timestamp":1763818400176,"user_tz":-60,"elapsed":139,"user":{"displayName":"José Sánchez","userId":"06686304146576226957"}},"outputId":"27c6ab59-14e3-4cfb-b937-83651b90025a"},"execution_count":42,"outputs":[{"output_type":"stream","name":"stdout","text":["Replaced 1047611 weather delays between 4.03 and 4.04 with random values (mean=15, std=5).\n"]}]},{"cell_type":"code","source":["import pandas as pd\n","import requests\n","from io import StringIO\n","\n","# -----------------------------\n","# 1. Download the airports data\n","# -----------------------------\n","url = \"https://ourairports.com/data/airports.csv\"\n","csv_data = requests.get(url).text\n","airports = pd.read_csv(StringIO(csv_data))\n","\n","# -----------------------------\n","# 2. Your list of IATA codes\n","# -----------------------------\n","iata_codes = {\n"," 'ABE','ABI','ABQ','ABR','ABY','ACK','ACT','ACV','ACY','ADK','ADQ','AEX','AGS','AKN','ALB','ALO',\n"," 'ALW','AMA','ANC','APN','ART','ASE','ATL','ATW','ATY','AUS','AVL','AVP','AZA','AZO','BDL','BET',\n",