n","]\n","\n","# Filter for January 2020\n","january_2020_flights_airlines = df_with_airline_names[\n","    (df_with_airline_names['FL_DATE'].dt.year == 2020) &\n","    (df_with_airline_names['FL_DATE'].dt.month == 1)\n","]\n","\n","# Calculate total flights per airline for August 2018\n","august_2018_top_airlines = august_2018_flights_airlines.groupby('Airline_Name')['FL_DATE'].count().reset_index()\n","august_2018_top_airlines = august_2018_top_airlines.rename(columns={'FL_DATE': 'Total_Flights'})\n","august_2018_top_airlines['Period'] = 'August 2018'\n","\n","# Calculate total flights per airline for January 2020\n","january_2020_top_airlines = january_2020_flights_airlines.groupby('Airline_Name')['FL_DATE'].count().reset_index()\n","january_2020_top_airlines = january_2020_top_airlines.rename(columns={'FL_DATE': 'Total_Flights'})\n","january_2020_top_airlines['Period'] = 'January 2020'\n","\n","# Combine and get top N airlines for visualization (e.g., top 10 overall)\n","combined_airlines = pd.concat([august_2018_top_airlines, january_2020_top_airlines])\n","\n","# Get the top 10 airlines based on total flights across both periods\n","top_10_airline_names = combined_airlines.groupby('Airline_Name')['Total_Flights'].sum().nlargest(10).index\n","\n","# Filter combined_airlines to only include these top 10\n","filtered_combined_airlines = combined_airlines[combined_airlines['Airline_Name'].isin(top_10_airline_names)]\n","\n","# Create the interactive bar chart using Plotly Express\n","fig = px.bar(data_frame=filtered_combined_airlines,\n","             x='Airline_Name',\n","             y='Total_Flights',\n","             color='Period',\n","             barmode='group',\n","             title='Top Airlines by Flight Volume: August 2018 vs. January 2020',\n","             labels={'Total_Flights': 'Total Number of Flights'})\n","\n","fig.update_layout(xaxis_title='Airline Name', yaxis_title='Total Number of Flights')\n","fig.show()"],"execution_count":79,"outputs":[